{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb-base",
    "plugin:react/recommended",
    "eslint:recommended"
  ],
  "plugins": [
    "react",
    "jest"
  ],
  "rules": {
    "no-console":"off",
    "import/no-unresolved": [
      2,
      { "caseSensitive": false }
   ]
  },
  "settings":{
  "import/resolver": "webpack"
}
}
